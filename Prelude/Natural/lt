{-
`lt` checks if one Natural is strictly less than another.

Examples:

```
./lt 5 6
= True

./lt 5 5
= False

./lt 5 4
= False
```
-}
let lte =
        ./lte sha256:1a5caa2b80a42b9f58fff58e47ac0d9a9946d0b2d36c54034b8ddfe3cb0f3c99
      ? ./lte

let lt
    : Natural → Natural → Bool
    = λ(x : Natural) → λ(y : Natural) → lte (x + 1) y

in  lt
