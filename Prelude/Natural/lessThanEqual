
{-
`lessThan` checks if one Natural is less than or equal to another.

Examples:

```
./lessThanEqual 5 6
= True

./lessThanEqual 5 5
= True

./lessThanEqual 5 4
= False
```
-}
let lessThanEqual
    : Natural → Natural → Bool
    = λ(x : Natural) → λ(y : Natural) → Natural/isZero (Natural/subtract y x)

in  lessThanEqual
