{-
`equal` checks if two Naturals are equal.

Examples:

```
./equal 5 5
= True

./equal 5 6
= False
```
-}
let equal
    : Natural → Natural → Bool
    =   λ(a : Natural)
      → λ(b : Natural)
      →     Natural/isZero (Natural/subtract a b)
        &&  Natural/isZero (Natural/subtract b a)

in  equal
